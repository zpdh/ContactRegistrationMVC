@model ContactRegistrationMVC.Models.UserModel

@{
    ViewData["Title"] = "Delete Contact";
}
<div class="text-center">
    <h4>Are you sure you want to delete this contact?</h4>
</div>
<hr />
<dl class="row">
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Name)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Name)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Login)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Login)
    </dd>
    <dt class="col-sm-2">
        @Html.DisplayNameFor(model => model.Email)
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.Email)
    </dd>
    <dt class="col-sm-2">
        User type
    </dt>
    <dd class="col-sm-10">
        @Html.DisplayFor(model => model.UserType)
    </dd>
</dl>
<div class="mt-4">
    <a asp-action="Del" asp-route-id="@Model.Id" class="btn btn-danger" role="button" />Delete</a>
    <a asp-action="Index" role="button" class="btn btn-secondary">Cancel</a>
</div>